{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Python: Special Module",
			"type": "python",
			"request": "launch",
			"cwd": "/home/imxood/develop/sources/zephyrproject/.west/west/src",
			"pythonPath": "/usr/bin/python3",
			"stopOnEntry": true,
			"module": "west.main",
			"args": [
				"--verbose",
				"flash",
				"--skip-rebuild",
				"--build-dir",
				"~/develop/sources/zephyrproject/zephyr/zephyr_output"
			],
			"console": "integratedTerminal"
		},
		{
			"type": "esp32-debug",
			"request": "launch",
			"name": "esp32-debug",
			"program": "${workspaceFolder}/zephyr_output/zephyr/zephyr.elf",
			"stopOnEntry": true
		},
		{
			"cwd": "${workspaceRoot}",
			"executable": "/tmp/arduino_build_476409/main.ino.elf",
			"name": "Remote Arduino",
			"request": "launch",
			"type": "cortex-debug",
			"servertype": "external",
			"armToolchainPath": "/home/mx/programs/gcc-arm-none-eabi-9-2019-q4-major/bin",
			"gdbTarget": "10.239.144.210:3333",
			"preLaunchCommands": [
				"monitor halt"
			]
		},
		{
			"type": "cortex-debug_stm32f7",
			"request": "launch",
			"servertype": "openocd",
			"cwd": "${workspaceRoot}",
			"executable": "./w_output/zephyr/zephyr.elf",
			"name": "Stm32f7 Debug",
			"device": "STM32F7",
			"configFiles": [
				"board/stm32f7discovery.cfg"
			]
		},
		{
			"cwd": "${workspaceRoot}",
			"executable": "/tmp/arduino_build_242531/main.ino.elf",
			"name": "Debug Arduino",
			"request": "launch",
			"type": "cortex-debug",
			"servertype": "openocd",
			"armToolchainPath": "/home/mx/programs/gcc-arm-none-eabi-9-2019-q4-major/bin",
			"configFiles": [
				"/usr/local/share/openocd/scripts/board/stm32f7discovery.cfg"
			]
		},
		{
			"name": "stm32 debug",
			"type": "cppdbg",
			"request": "launch",
			"targetArchitecture": "arm",
			"program": "${workspaceFolder}/build/${workspaceFolderBasename}.elf",
			"args": [],
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}",
			"environment": [],
			"externalConsole": false,
			"MIMode": "gdb",
			"miDebuggerPath": "${env:HOME}/programs/gcc-arm-none-eabi-7/bin/arm-none-eabi-gdb",
			"setupCommands": [
				{
					"description": "Enable pretty-printing for gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				},
				{
					"text": "file ${workspaceFolder}/build/${workspaceFolderBasename}.elf"
				},
				{
					"text": "set remotetimeout 30"
				},
				{
					"text": "target remote localhost:3333"
				},
				{
					"text": "monitor halt"
				},
				{
					"text": "monitor reset init"
				},
				{
					"text": "load"
				},
				{
					"text": "info target"
				}
			]
		}
	]
}
